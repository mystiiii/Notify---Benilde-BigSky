{
  "name": "notify-desktop",
  "version": "1.0.0",
  "description": "Benilde BigSky Automation Tool",
  "main": "electron.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run start-backend\" \"npm run start-frontend\" \"wait-on http://localhost:5173 && electron .\"",
